generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Website {
  id             String   @id @default(uuid())
  url            String
  videoSourceUrl String
  categories     String[]
  notes          String?
  title          String?
  description    String?
  image          String?
  createdAt      DateTime @default(now())
}